using System;

using Nemerle;
using Nemerle.Compiler;
using Nemerle.Compiler.Parsetree;

namespace LinqToDB_Temp.MacroInternal
{
	module Tools
	{
		public ToName(this expr : PExpr) : Name
		{
			| <[ $(name : name) ]> => name
			| _ => Message.FatalError(expr.Location, "Expected simple name.");
		}
	}
}
